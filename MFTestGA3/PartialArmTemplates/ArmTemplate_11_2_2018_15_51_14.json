{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "MFTestGA3"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Wait1",
						"type": "Wait",
						"typeProperties": {
							"waitTimeInSeconds": 20
						}
					}
				],
				"parameters": {
					"p1": {
						"type": "String",
						"defaultValue": "a"
					},
					"p2": {
						"type": "String",
						"defaultValue": "b"
					},
					"p3": {
						"type": "String",
						"defaultValue": "c"
					},
					"p4": {
						"type": "String",
						"defaultValue": "d"
					},
					"p5": {
						"type": "String",
						"defaultValue": "e"
					},
					"p6": {
						"type": "String",
						"defaultValue": "f"
					},
					"p7": {
						"type": "String",
						"defaultValue": "g"
					},
					"p8": {
						"type": "String",
						"defaultValue": "h"
					},
					"p9": {
						"type": "String",
						"defaultValue": "i"
					},
					"p10": {
						"type": "String",
						"defaultValue": "j"
					}
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/pipeline2')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Web1",
						"type": "WebActivity",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"url": "http://foo.com",
							"method": "GET"
						}
					}
				],
				"parameters": {
					"param1": {
						"type": "String",
						"defaultValue": "aa"
					},
					"param2": {
						"type": "String",
						"defaultValue": "bb"
					},
					"param3": {
						"type": "String",
						"defaultValue": "cc"
					},
					"param4": {
						"type": "String",
						"defaultValue": "dd"
					},
					"param5": {
						"type": "String",
						"defaultValue": "ee"
					},
					"param6": {
						"type": "String",
						"defaultValue": "ff"
					},
					"param7": {
						"type": "String",
						"defaultValue": "gg"
					},
					"param8": {
						"type": "String",
						"defaultValue": "hh"
					},
					"param9": {
						"type": "String",
						"defaultValue": "ii"
					},
					"param10": {
						"type": "String",
						"defaultValue": "jj"
					}
				}
			}
		}
	]
}